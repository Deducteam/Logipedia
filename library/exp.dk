def sym_eq_exp :
  sttfa.eps 
    (sttfa.forall nat.nat 
       (n:(sttfa.etap (sttfa.p nat.nat)) =>
        sttfa.forall nat.nat 
          (m:(sttfa.etap (sttfa.p nat.nat)) =>
           sttfa.leibniz nat.nat 
             (nat.filter_nat_type nat.nat (exp.exp_body n) m) (exp.exp n m))))
  :=
  n:(sttfa.etap (sttfa.p nat.nat)) =>
